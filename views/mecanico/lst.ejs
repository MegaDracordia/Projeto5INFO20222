<%- include('../cabecalho.ejs') %>
    <form method="post">
        <input type="text" name="pesquisar">
        <input type="submit" value="Pesquisar" class="btn btn-primary">
    </form>

    <table>
        <tr>
            <th style="text-align: justify">Id</th>
            <th style="text-align: justify">Nome</th>
            <th style="text-align: justify">Email</th>
            <th style="text-align: justify">Celular</th>
            <th style="text-align: justify">CPF</th>
            <th style="text-align: justify">Foto</th>
            <th style="text-align: justify">Editar</th>
            <th style="text-align: justify">Deletar</th>
        </tr>
        <% Mecanicos.forEach(function(mecanico){%>
        <tr>
            <td style="text-align: justify"><%=mecanico._id %></td>
            <td style="text-align: justify"><%=mecanico.nome %></td>
            <td style="text-align: justify"><%=mecanico.email %></td>
            <td style="text-align: justify"><%=mecanico.celular %></td>
            <td style="text-align: justify"><%=mecanico.cpf %></td>
            <td style="text-align: justify"><img style="max-width: 100px; max-height: 100px;" src="/fotos/<%=mecanico.foto%>" alt="Foto do MecÃ¢nico"></td>
            <td style="text-align: justify"><a href="/admin/mecanico/edt/<%=mecanico.id %>">Editar</a></td>
            <td style="text-align: justify"><a href="/admin/mecanico/del/<%=mecanico.id %>">Excluir</a></td>
        </tr>
        <% })%>
    </table>
